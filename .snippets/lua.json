{
  "Function": {
    "prefix": "func",
    "body": [
      "function $1()",
      "$0",
      "end"
    ]
  },
  "Local Function": {
    "prefix": "lfunc",
    "body": [
      "local function $1()",
      "$0",
      "end"
    ]
  },
  "LuaDoc url": {
    "prefix": "doc-url",
    "body": "---@url $1$0"
  },
  "LuaDoc url ï‚› GitHub": {
    "prefix": "doc-url-gh",
    "body": "---@url https://github.com/$0"
  },
  "LuaDoc type": {
    "prefix": "doc-type",
    "body": "---@type $1$0"
  },
  "LuaDoc type LazyPluginSpec": {
    "prefix": "doc-type-lazypluginspec",
    "body": "---@type LazyPluginSpec$0"
  },
  "Load Telescope Extension": {
    "prefix": "tele-load-ext",
    "body": "require('telescope').load_extension('$1')$0"
  },
  "Lazy Telescope Package": {
    "prefix": "lazy-pkg-tele",
    "body": "'nvim-telescope/telescope.nvim'"
  },
  "Lazy Snacks Package": {
    "prefix": "lazy-pkg-snacks",
    "body": "'folke/snacks.nvim'"
  },
  "Lazy Flexoki Package": {
    "prefix": "lazy-pkg-flexoki",
    "body": "'kepano/flexoki-neovim'"
  },
  "Lazy Fidget Package": {
    "prefix": "lazy-pkg-fidget",
    "body": "'j-hui/fidget.nvim'"
  },
  "Lazy Keymap": {
    "prefix": "lazy-keymap",
    "body": [
      "{",
      "'$1',",
      "$0,",
      "'$2',",
      "desc = '$3',",
      "},"
    ]
  },
  "New Snacks Toggle": {
    "prefix": "new-snacks-toggle",
    "body": [
      "Snacks.toggle.new(",
      "---@type snacks.toggle.Opts",
      "{",
      "id = '$1',",
      "name = '$2',",
      "get = function()",
      "---@TODO: implement",
      "end,",
      "set = function(value)",
      "---@TODO: implement",
      "end,",
      "}",
      ")$0"
    ]
  },
  "Neovim autocmd by Filetype": {
    "prefix": "nvim-autocmd-by-filetype",
    "body": [
      "vim.api.nvim_create_autocmd('FileType', {",
      "pattern = { $1 },",
      "callback = function()",
      "$0",
      "end,",
      "})"
    ]
  }
}
